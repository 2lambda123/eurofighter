<?xml version="1.0"?>

<!--
************************************************************************
Eurofighter Typhoon simulation config.

By MaverickAlex, D. Faber, AlMurSi & Algernon
************************************************************************
-->
<PropertyList>
	<sim include="typhoon-config.xml">
		<description>EFA (Eurofighter) Typhoon</description>
		<author>Maverick Alex and others</author>
		<status>Special Edition</status>
		<aircraft-version>1.5X</aircraft-version>
		<flight-model>jsb</flight-model>
		<aero>typhoon-jsbsim</aero>
		<fuel-fraction>0.6</fuel-fraction>
		<startup>
			<splash-texture>Aircraft/eurofighter/Splash2.jpg</splash-texture>
		</startup>
		<preferences>
			<start-running type="bool">true</start-running>
		</preferences>

		<gui>
			<dialogs>
				<radios>
					<transponder-mode type="string">ON</transponder-mode>
				</radios>
			</dialogs>
		</gui>

		<model>
			<path>Aircraft/eurofighter/Models/Typhoon.xml</path>
			<livery>
				<file type="string"/>
				<RAFlights type="bool"/>
				<beacon type="bool"/>
				<strobe type="bool"/>
			</livery>
		</model>
		<!--<help include="gui/typhoon-help.xml"></help>
		<menubar include="Dialogs/typhoon-menu.xml"></menubar>-->
		<menubar>
			<default>
				<menu n="101">
					<label>Dev</label>
					<item>
						<label>Tune Drag</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_drag_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Lift</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_lift_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Pitch</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_pitch_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Roll</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_roll_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Side</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_side_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Yaw</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_yaw_dialog.open()</script>
						</binding>
					</item>
					<item>
						<name>a-submenu</name>
						<enabled>false</enabled>
					</item>
					<item>
						<label>Dump Prop Tree</label>
						<binding>
							<command>nasal</command><![CDATA[some stuff]]>
							<script>
								io.write_properties( path: getprop("/sim/fg-home") ~ "/Export/MiG-21bis/prop_tree_dump.xml", prop: "/" );
							</script>
						</binding>
					</item>
				</menu>
			</default>
		</menubar>
		<wildlife>
			<birdsong type="bool">true</birdsong>
			<birdsong-volume type="double">0.75</birdsong-volume>
		</wildlife>
		<rendering>
			<als-secondary-lights>
		        <landing-light1-offset-deg type="double">0</landing-light1-offset-deg>
		        <landing-light2-offset-deg type="double">0</landing-light2-offset-deg>
		        <landing-light3-offset-deg type="float">0</landing-light3-offset-deg>
			</als-secondary-lights>
            <shadow-volume type="bool">true</shadow-volume>
		</rendering>
	</sim>

	<controls><canopy><position-norm>0</position-norm></canopy></controls>

	<!--<systems include="typhoon-systems.xml" />-->
	<identification>
		<aircraft-number type="string" archive="y">XP-210</aircraft-number>
		<pilot-name type="string" archive="y"></pilot-name>
	</identification>
	<input>
		<keyboard include="Input/typhoon-keyboard.xml" />
	</input>
	<!--canopy>
		<position-norm type="double">1</position-norm>
	</canopy-->
	<computers>
		<navigation>
			<mode type="string">bug</mode>
		</navigation>
	</computers>
	<consumables>
		<fuel>
			<tank n="0">
				<name>forward fuselage Tank</name>
				<selected type="bool">true</selected>
				<kill-when-empty type="bool">false</kill-when-empty>
				<level-gal_us archive="y">500</level-gal_us>
			</tank>
			<tank n="1">
				<name>rear fuselage Tank</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="2">
				<name>forward left wing Tank</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="3">
				<name>rear left wing Tank</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="4">
				<name>forward right wing Tank</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="5">
				<name>rear right wing Tank</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="6">
				<name>left wing Droptank</name>
				<selected type="bool">false</selected>
			</tank>
			<tank n="7">
				<name>right wing Droptank</name>
				<selected type="bool">false</selected>
			</tank>
			<tank n="8">
				<name>centerline Droptank</name>
				<selected type="bool">false</selected>
			</tank>
			<dump-valve type="bool">false</dump-valve>
		</fuel>
	</consumables>
	<engines>
		<engine n="0"><running>true</running></engine>
		<engine n="1"><running>true</running></engine>
	</engines>

	<fdm>
		<jsbsim>
			<systems>
				<electric>
					<input>
						<!-- switches and 2-state buttons-->
						<battery type="double">0</battery>
						<lboost type="double">0</lboost>
						<rboost type="double">0</rboost>
						<voice type="double">0</voice>
						<rad1 type="double">0</rad1>
						<mids type="double">0</mids>
						<rlpcock type="double">0</rlpcock>
						<llpcock type="double">0</llpcock>
						<enginestart type="double">0</enginestart>
						<apu type="double">0</apu>
						<aflash type="double">0</aflash> <!-- beacon on top center spine -->
						<daynightlight type="double">0</daynightlight>
						<extlight type="double">0</extlight>
						<navbrt type="double">0</navbrt>
						<acoll type="double">0</acoll>
						<ecsramair type="double">0</ecsramair>
						<demist type="double">0</demist>
						<decoy type="double">0</decoy>
						<expdman type="double">0</expdman>
						<emergoxy type="double">0</emergoxy>
						<landtaxioff type="double">0</landtaxioff>
					</input>
					<output>
						<!-- switches and 2-state buttons-->
						<battery type="double">0</battery>
						<lboost type="double">0</lboost>
						<rboost type="double">0</rboost>
						<voice type="double">0</voice>
						<rad1 type="double">0</rad1>
						<mids type="double">0</mids>
						<rlpcock type="double">0</rlpcock>
						<llpcock type="double">0</llpcock>
						<enginestart type="double">0</enginestart>
						<apu type="double">0</apu>
						<aflash type="double">0</aflash> <!-- beacon on top center spine -->
						<daynightlight type="double">0</daynightlight>
						<extlight type="double">0</extlight>
						<navbrt type="double">0</navbrt>
						<acoll type="double">0</acoll>
						<ecsramair type="double">0</ecsramair>
						<demist type="double">0</demist>
						<decoy type="double">0</decoy>
						<expdman type="double">0</expdman>
						<emergoxy type="double">0</emergoxy>
						<landtaxioff type="double">0</landtaxioff>
						<!--knobbins-->
					</output>
				</electric>
			</systems>




			<!-- for in-flight FDM tuning -->
			<tune>
				<lift>
					<alpha type="double">1</alpha>
					<pitch_rate type="double">1</pitch_rate>
					<alpha_rate type="double">1</alpha_rate>
					<elevator type="double">1</elevator>
				</lift>
				<drag>
					<minimum type="double">1</minimum>
					<alpha type="double">1</alpha>
					<induced type="double">1</induced>
					<mach type="double">1</mach>
					<beta type="double">1</beta>
					<elevator type="double">1</elevator>
					<gear type="double">1</gear>
					<flap type="double">1</flap>
					<speedbrake type="double">1</speedbrake>
					<chute type="double">1</chute>
				</drag>
				<side>
					<beta type="double">1</beta>
					<roll_rate type="double">1</roll_rate>
					<yaw_rate type="double">1</yaw_rate>
					<rudder type="double">1</rudder>
				</side>
				<pitch>
					<alpha type="double">1</alpha>
					<elevator type="double">1</elevator>
					<damp type="double">1</damp>
					<alphadot type="double">1</alphadot>
				</pitch>
				<roll>
					<beta type="double">1</beta>
					<damp type="double">1</damp>
					<yaw type="double">1</yaw>
					<aileron type="double">1</aileron>
					<rudder type="double">1</rudder>
				</roll>
				<yaw>
					<alpha type="double">1</alpha>
					<beta type="double">1</beta>
					<roll_rate type="double">1</roll_rate>
					<damp type="double">1</damp>
					<rudder type="double">1</rudder>
					<aileron type="double">1</aileron>
				</yaw>
			</tune>
		</jsbsim>
	</fdm>

<!-- Nasal files/scripts -->
	<nasal>
		<dialog> <file>Aircraft/eurofighter/Nasal/dialogs.nas</file> </dialog>
		
	</nasal>
	
</PropertyList>
