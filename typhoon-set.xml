<?xml version="1.0"?>

<!--
************************************************************************
Eurofighter Typhoon simulation config.

By MaverickAlex, D. Faber, AlMurSi & Algernon
************************************************************************
-->
<PropertyList>
	<sim include="typhoon-config.xml">
		<description>EFA (Eurofighter) Typhoon</description>
		<author>Maverick Alex, Justin Nicholson (pinto), Joshua Davidson (it0uchpods), and others</author>
		<status>Special Edition</status>
		<aircraft-version>1.5X</aircraft-version>
		<flight-model>jsb</flight-model>
		<aero>typhoon-jsbsim</aero>
		<startup>
			<splash-texture>Aircraft/eurofighter/Splash2.jpg</splash-texture>
		</startup>
		<preferences>
			<start-running type="bool">true</start-running>
		</preferences>

		<gui>
			<dialogs>
				<radios>
					<transponder-mode type="string">ON</transponder-mode>
				</radios>
			</dialogs>
		</gui>

		<model>
			<path>Aircraft/eurofighter/Models/Typhoon.xml</path>
			<livery>
				<file type="string"/>
				<RAFlights type="bool"/>
				<beacon type="bool"/>
				<strobe type="bool"/>
			</livery>
		</model>
		<!--<help include="gui/typhoon-help.xml"></help>
		<menubar include="Dialogs/typhoon-menu.xml"></menubar>-->
		<menubar>
			<default>
				<menu n="101">
					<label>Dev</label>
					<item>
						<label>FBW Control</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>fbw</dialog-name>
						</binding>
					</item>
					<item>
						<label>Tune Drag</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_drag_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Lift</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_lift_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Pitch</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_pitch_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Roll</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_roll_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Side</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_side_dialog.open()</script>
						</binding>
					</item>
					<item>
						<label>Tune Yaw</label>
						<binding>
							<command>nasal</command>
							<script>dialog.tuner_yaw_dialog.open()</script>
						</binding>
					</item>
					<item>
						<name>a-submenu</name>
						<enabled>false</enabled>
					</item>
					<item>
						<label>Dump Prop Tree</label>
						<binding>
							<command>nasal</command><![CDATA[some stuff]]>
							<script>
								io.write_properties( path: getprop("/sim/fg-home") ~ "/Export/MiG-21bis/prop_tree_dump.xml", prop: "/" );
							</script>
						</binding>
					</item>
				</menu>
			</default>
		</menubar>
		<wildlife>
			<birdsong type="bool">true</birdsong>
			<birdsong-volume type="double">0.75</birdsong-volume>
		</wildlife>
		<rendering>
			<als-secondary-lights>
		        <landing-light1-offset-deg type="double">0</landing-light1-offset-deg>
		        <landing-light2-offset-deg type="double">0</landing-light2-offset-deg>
		        <landing-light3-offset-deg type="float">0</landing-light3-offset-deg>
			</als-secondary-lights>
            <shadow-volume type="bool">true</shadow-volume>
		</rendering>
		
		<flaps>
			<setting>0.0</setting>
			<setting>0.5</setting>
			<setting>1.0</setting>
		</flaps>
	</sim>

	<controls>
		<canopy>
			<position-norm>0</position-norm>
		</canopy>
	</controls>

	<!--<systems include="typhoon-systems.xml" />-->
	<identification>
		<aircraft-number type="string" archive="y">XP-210</aircraft-number>
		<pilot-name type="string" archive="y"></pilot-name>
	</identification>
	<input n="0">
		<mice n="0">
			<mouse n="0">
				<mode n="0">
					<button n="3">
						<binding n="0">
							<script></script>
						</binding>
						<binding n="1">
							<condition>
								<and>
									<property>/devices/status/mice/mouse[0]/button[2]</property>
									<not><property>/sim/mouse/right-button-mode-cycle-enabled</property></not>
								</and>
							</condition>
							<command>nasal</command>
							<script>libraries.fovZoom(-1);</script>
						</binding>
					</button>
					<button n="4">
						<binding n="0">
							<script></script>
						</binding>
						<binding n="1">
							<condition>
								<and>
									<property>/devices/status/mice/mouse[0]/button[2]</property>
									<not><property>/sim/mouse/right-button-mode-cycle-enabled</property></not>
								</and>
							</condition>
							<command>nasal</command>
							<script>libraries.fovZoom(1);</script>
						</binding>
					</button>
				</mode>
			</mouse>
		</mice>
		<keyboard include="Input/typhoon-keyboard.xml" />
	</input>
	<!--canopy>
		<position-norm type="double">1</position-norm>
	</canopy-->
	<computers>
		<navigation>
			<mode type="string">bug</mode>
		</navigation>
	</computers>
	<consumables>
		<fuel>
			<tank n="0">
				<name>Fore</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="1">
				<name>Mid</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="2">
				<name>Aft</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="3">
				<name>Foreleft Wing</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="4">
				<name>Aftleft Wing</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="5">
				<name>Foreright Wing</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="6">
				<name>Aftright Wing</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="7">
				<name>Center Droptank</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="8">
				<name>Left Droptank</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="9">
				<name>Right Droptank</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="10">
				<name>Left Accumulator</name>
				<selected type="bool">true</selected>
			</tank>
			<tank n="11">
				<name>Right Accumulator</name>
				<selected type="bool">true</selected>
			</tank>
			<dump-valve type="bool">false</dump-valve>
		</fuel>
	</consumables>
	
	<engines>
		<engine n="0">
			<running>true</running>
		</engine>
		<engine n="1">
			<running>true</running>
		</engine>
		<engine n="2"> <!-- A crude "hack" to send slats over MP, because we are out of floats -->
			<n1 type="double">0</n1> <!-- L -->
			<n2 type="double">0</n2> <!-- R -->
		</engine>
	</engines>

	<fdm>
		<jsbsim>
			<systems>
				<apu>
					<spinrate type="double">0</spinrate>
					<switch type="double">0</switch>
					<power type="double">0</power>
				</apu>
				<electric>
					<sources>
						<ext-power-connected type="double">0</ext-power-connected>
						<ext-power-norm type="double">0</ext-power-norm>
						<engine0-norm type="double">0</engine0-norm>
						<engine1-norm type="double">0</engine1-norm>
						<apu-norm type="double">0</apu-norm>
						<battery-pre type="double">0.95</battery-pre>
						<battery-norm type="double">0</battery-norm>
						<battery-drain type="double">0</battery-drain>
						<battery-switch type="double">0</battery-switch>
						<output-norm type="double">0</output-norm>
					</sources>

					<input>
						<!-- switches and 2-state buttons-->
						<battery type="double">0</battery>
						<lboost type="double">0</lboost>
						<rboost type="double">0</rboost>
						<voice type="double">0</voice>
						<rad1 type="double">0</rad1>
						<mids type="double">0</mids>
						<rlpcock type="double">0</rlpcock>
						<llpcock type="double">0</llpcock>
						<enginestart type="double">0</enginestart>
						<apu type="double">0</apu>
						<aflash type="double">0</aflash> <!-- beacon on top center spine -->
						<daynightlight type="double">0</daynightlight>
						<extlight type="double">0</extlight>
						<navbrt type="double">0</navbrt>
						<acoll type="double">0</acoll>
						<ecsramair type="double">0</ecsramair>
						<demist type="double">0</demist>
						<decoy type="double">0</decoy>
						<expdman type="double">0</expdman>
						<emergoxy type="double">0</emergoxy>
						<landtaxioff type="double">0</landtaxioff>
					</input>
					<output>
						<!-- switches and 2-state buttons-->
						<battery type="double">0</battery>
						<lboost type="double">0</lboost>
						<rboost type="double">0</rboost>
						<voice type="double">0</voice>
						<rad1 type="double">0</rad1>
						<mids type="double">0</mids>
						<rlpcock type="double">0</rlpcock>
						<llpcock type="double">0</llpcock>
						<enginestart type="double">0</enginestart>
						<apu type="double">0</apu>
						<aflash type="double">0</aflash> <!-- beacon on top center spine -->
						<daynightlight type="double">0</daynightlight>
						<extlight type="double">0</extlight>
						<navbrt type="double">0</navbrt>
						<acoll type="double">0</acoll>
						<ecsramair type="double">0</ecsramair>
						<demist type="double">0</demist>
						<decoy type="double">0</decoy>
						<expdman type="double">0</expdman>
						<emergoxy type="double">0</emergoxy>
						<landtaxioff type="double">0</landtaxioff>
						<!--knobbins-->
					</output>
					<!-- replace me -->
					<left-booster type="double">1</left-booster>
					<right-booster type="double">1</right-booster>
					<transfer-boosters type="double">1</transfer-boosters>
				</electric>
				<hydraulic>
					<contents-tank type="double" n="0">0</contents-tank>
					<contents-system type="double" n="0">1</contents-system>
					<pressure type="double" n="0">0</pressure>
					<usage type="double" n="0">0</usage>
					<output type="double" n="0">0</output>
					<contents-tank type="double" n="1">0</contents-tank>
					<contents-system type="double" n="1">1</contents-system>
					<pressure type="double" n="1">0</pressure>
					<usage type="double" n="1">0</usage>
					<output type="double" n="1">0</output>
					<xfeed type="double">0</xfeed>
				</hydraulic>
				<fuel>
					<pressure-flow type="double">0</pressure-flow>
					<transfer-flow type="double">0</transfer-flow>
					<left-boost-pressure type="double">0</left-boost-pressure>
					<right-boost-pressure type="double">0</right-boost-pressure>
					<transfer-pressure type="double">0</transfer-pressure>
					<left-lpcock type="double">1</left-lpcock>
					<right-lpcock type="double">1</right-lpcock>
					<xfeed type="double">1</xfeed>
					<cg-ratio type="double">0.5</cg-ratio>
				</fuel>

			</systems>




			<!-- for in-flight FDM tuning -->
			<tune>
				<lift>
					<alpha type="double">1</alpha>
					<pitch_rate type="double">1</pitch_rate>
					<alpha_rate type="double">1</alpha_rate>
					<elevator type="double">1</elevator>
				</lift>
				<drag>
					<minimum type="double">1</minimum>
					<alpha type="double">1</alpha>
					<induced type="double">1</induced>
					<mach type="double">1</mach>
					<beta type="double">1</beta>
					<elevator type="double">1</elevator>
					<gear type="double">1</gear>
					<flap type="double">1</flap>
					<speedbrake type="double">1</speedbrake>
					<chute type="double">1</chute>
				</drag>
				<side>
					<beta type="double">1</beta>
					<roll_rate type="double">1</roll_rate>
					<yaw_rate type="double">1</yaw_rate>
					<rudder type="double">1</rudder>
				</side>
				<pitch>
					<alpha type="double">1</alpha>
					<elevator type="double">1</elevator>
					<damp type="double">1</damp>
					<alphadot type="double">1</alphadot>
				</pitch>
				<roll>
					<beta type="double">1</beta>
					<damp type="double">1</damp>
					<yaw type="double">1</yaw>
					<aileron type="double">1</aileron>
					<rudder type="double">1</rudder>
				</roll>
				<yaw>
					<alpha type="double">1</alpha>
					<beta type="double">1</beta>
					<roll_rate type="double">1</roll_rate>
					<damp type="double">1</damp>
					<rudder type="double">1</rudder>
					<aileron type="double">1</aileron>
				</yaw>
			</tune>
		</jsbsim>
	</fdm>

	<velocities n="0">
		<groundspeed-kt>0</groundspeed-kt>
	</velocities>

<!-- Nasal files/scripts -->
	<nasal>
		<dialog> 
			<file>Aircraft/eurofighter/Nasal/dialogs.nas</file>
		</dialog>
		<libraries>
			<file>Aircraft/eurofighter/Nasal/zoom-views.nas</file>
		</libraries>
	</nasal>
	
</PropertyList>
