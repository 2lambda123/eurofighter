		<property>sim/multiplay/generic/string[0]</property>
		<draw-text type="bool">true</draw-text>
		<alignment>right-center</alignment>
	</text>

	<text>
		<name>acserial-left</name>
		<font>lucida.txf</font>
		<character-size type="double">0.17</character-size>
		<offsets>
			<x-m>12.05</x-m>
			<y-m>-1.02</y-m>
			<z-m>0.37</z-m>
			<roll-deg>-18</roll-deg>
			<heading-deg>0</heading-deg>
