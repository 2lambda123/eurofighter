<?xml version="1.0"?>
<PropertyList>
	<path>vii1.ac</path>

	<animation>
		<type>noshadow</type>
		<object-name>indaltlmob.001</object-name>
		<object-name>indalti.001</object-name>
	</animation>

	<animation> <!-- link electrical system -->
		<type>select</type>
		<object-name>indaltlmob.001</object-name>
		<object-name>indalti.001</object-name>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/avionics/ASI</property>
				<value>22</value>
			</greater-than>
		</condition>
	</animation>

	<animation> <!-- fixed show on 2.0 -->
		<object-name>indaltlmob.001</object-name>
		<object-name>indalti.001</object-name>
		<type>material</type>
		<diffuse>
			<red>0.9</red>
			<green>0.9</green>
			<blue>0.9</blue>
		</diffuse>
		<emission>
			<factor-prop>/instrumentation/illumination/glareshield-backlight</factor-prop>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
		</emission>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>indaltlmob.001</object-name>
		<property>velocities/vertical-speed-fps</property>
		<z-min>-0.02290</z-min>
		<z-max>0.02290</z-max>
		<interpolation>
			<entry><ind>-1000000 </ind><dep> -0.022900 </dep></entry>
			<entry><ind>   -2000 </ind><dep> -0.0228998 </dep></entry>
			<entry><ind>   -1000 </ind><dep> -0.02289975</dep></entry>
			<entry><ind>    -500 </ind><dep> -0.0228996 </dep></entry>
			<entry><ind>    -250 </ind><dep> -0.0228994 </dep></entry>
			<entry><ind>    -100 </ind><dep> -0.0228992 </dep></entry>
			<entry><ind>       0 </ind><dep>  0.000000 </dep></entry>
			<entry><ind>     100 </ind><dep>  0.0228992 </dep></entry>
			<entry><ind>     250 </ind><dep>  0.0228994 </dep></entry>
			<entry><ind>     500 </ind><dep>  0.0228996 </dep></entry>
			<entry><ind>    1000 </ind><dep>  0.02289975 </dep></entry>
			<entry><ind>    2000 </ind><dep>  0.0228998 </dep></entry>
			<entry><ind> 1000000 </ind><dep>  0.022900 </dep></entry>
		</interpolation>
		<axis>
			<x> 0 </x>
			<y> 0 </y>
			<z> 1 </z>
		</axis>
		<center>
			<x-m> 0 </x-m>
			<y-m> 0 </y-m>
			<z-m> 0 </z-m>
		</center>
	</animation>
</PropertyList>

