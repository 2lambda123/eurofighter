<?xml version="1.0"?>
<PropertyList>
	<path>fuelInfo.ac</path>
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<condition>
			<property>sim/rendering/rembrandt/enabled</property>
		</condition>
		<object-name>FuelInfo</object-name>
		<object-name>FuelInfo.lit</object-name>
	</effect>
	<animation>
		<type>select</type>
		<object-name>FuelInfo</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/avionics/cockpit-lighting</property>
				<value>24</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>FuelInfo.lit</object-name>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/avionics/cockpit-lighting</property>
				<value>24</value>
			</greater-than>
		</condition>
	</animation>
	<animation>
		<type>material</type>
		<object-name>FuelInfo.lit</object-name>
		<emission>
			<red>0.5</red>
			<green>0.5</green>
			<blue>0.5</blue>
			<factor-prop>/instrumentation/illumination/glareshield-backlight</factor-prop>
		</emission>
	</animation>
</PropertyList>