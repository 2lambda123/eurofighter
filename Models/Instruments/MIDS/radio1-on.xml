<?xml version="1.0" encoding="utf-8"?>

<PropertyList>

 <path>radio.ac</path>
 
 <text>
  <name>rad1-channel</name>
  <font>lcd.txf</font>
  <character-size type="double">0.0055</character-size>
  <offsets>
   <x-m>0.01</x-m>
   <y-m>0.047</y-m>
   <z-m>0.012</z-m>
  </offsets>
  <axis-alignment>yz-plane</axis-alignment>
  <type type="string">text-value</type>
  <property>/systems/CAMU/radio[0]/channel-number</property>
  <draw-text type="bool">true</draw-text>
  <alignment>center-center</alignment>
 </text>
 
 <text>
  <name>rad1-ident</name>
  <font>lcd.txf</font>
  <character-size type="double">0.00475</character-size>
  <offsets>
   <x-m>0.01</x-m>
   <y-m>0.0135</y-m>
   <z-m>0.012</z-m>
  </offsets>
  <axis-alignment>yz-plane</axis-alignment>
  <type type="string">text-value</type>
  <property>/systems/CAMU/radio[0]/identifier</property>
  <draw-text type="bool">true</draw-text>
  <alignment>left-center</alignment>
 </text>
 
 <text>
  <name>rad1-freq</name>
  <font>lcd.txf</font>
  <character-size type="double">0.00475</character-size>
  <offsets>
   <x-m>0.01</x-m>
   <y-m>0.0135</y-m>
   <z-m>0.005</z-m>
  </offsets>
  <axis-alignment>yz-plane</axis-alignment>
  <type type="string">number-value</type>
  <property>/systems/CAMU/radio[0]/frequency</property>
  <format>%3.2f</format>
  <draw-text type="bool">true</draw-text>
  <alignment>left-center</alignment>
 </text>
 
