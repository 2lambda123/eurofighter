<PropertyList>
	<path>mids-panel.ac</path>
	<background>Aircraft/eurofighter/Models/Instruments/MIDS/mids-screen.png</background>
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>20</max-m>
	</animation>
	<animation>
		<object-name>ac-number</object-name>
		<object-name>ac-callsign</object-name>
		<object-name>ac-model</object-name>
		<object-name>ac-bearing</object-name>
		<object-name>ac-distance</object-name>
		<object-name>ac-altitude</object-name>
		<object-name>ac-bearing1</object-name>
		<object-name>ac-distance1</object-name>
		<object-name>ac-altitude1</object-name>
		<object-name>zulu</object-name>
		<object-name>zulu-label</object-name>
		<object-name>localtime</object-name>
		<object-name>localtime-label</object-name>
		<object-name>localtime1</object-name>
		<object-name>localtime-label1</object-name>
		<object-name>localtime2</object-name>
		<object-name>localtime-label2</object-name>
		<type>material</type>
		<diffuse>
			<red>0.1</red>
			<green>0.3</green>
			<blue>0.1</blue>
		</diffuse>
		<emission>
			<red>0.4</red>
			<green>0.8</green>
			<blue>0.4</blue>
			<factor-prop>/instrumentation/illumination/glareshield-backlight</factor-prop>
		</emission>
	</animation>
 
