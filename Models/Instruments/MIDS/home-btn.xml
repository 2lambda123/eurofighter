<?xml version="1.0"?>
<PropertyList>
	<path>mids-panel.ac</path>
	<animation>
		<type>pick</type>
		<object-name>midsbutt.1</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script></script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>select</type>
		<object-name>midsbutt.4</object-name>
		<condition>
			<or>
			<equals>
				<property>/systems/CAMU/radio[0]/channel-number</property>
				<value>M1</value>
			</equals>
			<equals>
				<property>/systems/CAMU/radio[0]/channel-number</property>
				<value>M2</value>
			</equals>
			</or>
		</condition>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>midsbutt.4</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-swap</command>
				<property>/instrumentation/comm/frequencies/selected-mhz</property>
				<property>/instrumentation/comm/frequencies/standby-mhz</property>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>midsbutt.2</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script></script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>select</type>
		<object-name>midsbutt.5</object-name>
		<condition>
			<or>
			<equals>
				<property>/systems/CAMU/radio[1]/channel-number</property>
				<value>M1</value>
			</equals>
			<equals>
				<property>/systems/CAMU/radio[1]/channel-number</property>
				<value>M2</value>
			</equals>
			</or>
		</condition>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>midsbutt.5</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-swap</command>
				<property>/instrumentation/comm[1]/frequencies/selected-mhz</property>
				<property>/instrumentation/comm[1]/frequencies/standby-mhz</property>
			</binding>
		</action>
	</animation>
	<!--animation>
		<type>pick</type>
		<object-name>midsbutt.3</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>fgcom.radio.toggle_property_display();</script>
			</binding>
		</action>
	</animation-->
	<model>
		<name>home</name>
		<path>Aircraft/eurofighter/Models/Instruments/MIDS/home.xml</path>
		<offsets>
			<x-m>-0.002</x-m>
			<y-m>0.0</y-m>
			<z-m>0.00</z-m>
		</offsets>
	</model>

</PropertyList>
