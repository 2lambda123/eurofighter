   <!--GS in range-->
	<text>
		<name>gs-inrange</name>
		<offsets>
			<x-m>0.015</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.037</z-m>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type>literal</type>
		<text>GS IN RANGE</text>
		<font>helvetica_bold.txf</font>
		<character-size>0.005</character-size>
	</text>
	<animation>
		<type>select</type>
		<object-name>gs-inrange</object-name>
		<condition>
			<property>autopilot/internal/gs-in-range</property>
		</condition>
	</animation>
   
   <!--Altitude-->
	<text>
		<name>alt</name>
		<offsets>
			<x-m>0.015</x-m>
			<y-m>0.058</y-m>
			<z-m>0.010</z-m>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type>number-value</type>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<format>%5.0f</format>
		<truncate>true</truncate>
		<font>helvetica_bold.txf</font>
		<character-size>0.009</character-size>
	</text>
   
   <!--Vertical speed plus-->
	<text>
		<name>vs+</name>
		<offsets>
			<x-m>0.015</x-m>
			<y-m>0.057</y-m>
			<z-m>0.018</z-m>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type>number-value</type>
     <!--property>autopilot/internal/vert-speed-fpm</property-->
		<property>velocities/vertical-speed-fps</property>
		<scale>60</scale>
		<format>+%5.0f</format>
		<truncate>true</truncate>
		<font>helvetica_bold.txf</font>
		<character-size>0.008</character-size>
	</text>
	<animation>
		<type>select</type>
		<object-name>vs+</object-name>
		<condition>
			<greater-than>
				<property>velocities/vertical-speed-fps</property>
				<value>2</value>
			</greater-than>
		</condition>
	</animation>
   
   <!--Vertical speed moins-->
	<text>
		<name>vs-</name>
		<offsets>
			<x-m>0.015</x-m>
			<y-m>0.057</y-m>
			<z-m>0.002</z-m>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type>number-value</type>
     <!--property>autopilot/internal/vert-speed-fpm</property-->
		<property>velocities/vertical-speed-fps</property>
		<scale>60</scale>
		<format>%5.0f</format>
		<truncate>true</truncate>
		<font>helvetica_bold.txf</font>
		<character-size>0.008</character-size>
	</text>
	<animation>
		<type>select</type>
		<object-name>vs-</object-name>
		<condition>
			<less-than>
				<property>velocities/vertical-speed-fps</property>
				<value>-2</value>
			</less-than>
		</condition>
	</animation>
   
