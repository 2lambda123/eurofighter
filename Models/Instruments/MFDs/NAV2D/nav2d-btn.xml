		<object-name>MFDButtonLowbar.001</object-name>
		<condition>
			<equals>
				<property>/instrumentation/nd/display/arpt</property>
				<value>true</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>MFDButtonTopbar.002</object-name>
		<condition>
			<equals>
				<property>/instrumentation/nd/display/vor</property>
				<value>true</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>MFDButtonLowbar.002</object-name>
		<condition>
			<equals>
				<property>/instrumentation/nd/display/vor</property>
				<value>true</value>
