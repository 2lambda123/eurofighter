</animation-->
	<animation>
		<type>pick</type>
		<object-name>R2</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>instrumentation/tcas/serviceable</property>
			</binding>
			<binding>
				<command>property-cycle</command>
				<property>instrumentation/nd/display/tcas</property>
				<value>1</value>
				<value>0</value>
			</binding>
			<binding>
				<command>property-cycle</command>
				<property>instrumentation/primus2000/dc840/tcas</property>
				<value>1</value>
				<value>0</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>R3</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>setprop("/autopilot/route-manager/input", "@previous")</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>R4</object-name>
		<action>
			<button>0</button>
			<binding>
				<condition>
					<equals>
						<property>/autopilot/route-manager/active</property>
						<value>true</value>
					</equals>
				</condition>
				<command>nasal</command>
				<script>setprop("/autopilot/route-manager/input", "@next")</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>R5</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
      <binding>
        <command>property-cycle</command>
        <property>/instrumentation/efis/inputs/range-nm</property>
        <value>5</value>              
        <value>10</value>
        <value>20</value>
        <value>50</value>
        <value>100</value>
        <value>150</value>
      </binding>    
      <binding>
        <command>property-assign</command>
        <property>instrumentation/tcas/range</property>
        <property>/instrumentation/efis/inputs/range-nm</property>
      </binding> 
      <binding>
        <command>property-assign</command>
        <property>instrumentation/nd/range</property>
        <property>/instrumentation/efis/inputs/range-nm</property>              
      </binding> 
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>R6</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
      <binding>
        <command>property-cycle</command>
        <property>/instrumentation/efis/inputs/range-nm</property>
        <value>150</value>
        <value>100</value>
        <value>50</value>
        <value>20</value>
        <value>10</value>              
        <value>5</value>              
      </binding>    
      <binding>
        <command>property-assign</command>
        <property>instrumentation/tcas/range</property>
        <property>/instrumentation/efis/inputs/range-nm</property>
      </binding> 
      <binding>
        <command>property-assign</command>
        <property>instrumentation/nd/range</property>
        <property>/instrumentation/efis/inputs/range-nm</property>              
      </binding> 
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>knobL</object-name>
		<action>
			<button>4</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>/systems/autopilot/settings/target-heading-deg</property>
				<step>1</step>
				<min>0</min>
				<max>360</max>
				<wrap>true</wrap>
			</binding>
		</action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>Hdg</tooltip-id>
        <label>Heading Bug: %d</label>
        <property>/systems/autopilot/settings/target-heading-deg</property>
      </binding>
    </hovered>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>knobL</object-name>
		<action>
			<button>3</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>/systems/autopilot/settings/target-heading-deg</property>
				<step>-1</step>
				<min>0</min>
				<max>360</max>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>select</type>
		<object-name>MFDButtonTopbar.016</object-name>
		<condition>
			<equals>
				<property>/instrumentation/MFD[0]/page-selected</property>
				<value>nav2d</value>
			</equals>
		</condition>
	</animation>
	<animation>
