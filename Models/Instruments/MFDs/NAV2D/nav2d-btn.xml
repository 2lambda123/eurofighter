		<object-name>L5</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/MFD[0]/page-selected</property>
				<value>situana</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>L6</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/MFD[0]/page-selected</property>
				<value>eadi</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>C1</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>instrumentation/MFD[0]/page-selected</property>
