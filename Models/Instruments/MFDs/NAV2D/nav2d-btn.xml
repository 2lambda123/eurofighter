<?xml version="1.0" encoding="utf-8"?>
<PropertyList>
	<path>Aircraft/eurofighter/Models/Instruments/MFDs/mfd-buttons.ac</path>

	<animation>
		<type>pick</type>
		<object-name>L1</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>dialog-show</command>
				<dialog-name>map</dialog-name>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>L1</object-name>
		<action>
			<button>1</button>
			<binding>
				<command>dialog-close</command>
				<dialog-name>map</dialog-name>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>L2</object-name>
		<action>
			<button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/nd/display/arpt</property>
      </binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>L3</object-name>
		<action>
			<button>0</button>
