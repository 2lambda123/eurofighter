<?xml version="1.0"?>
<PropertyList>
	<path>eng-status.ac</path>
	
	<animation>
		<type>select</type>
		<object-name>rpm.L</object-name>
		<object-name>rpm1.L1</object-name>
		<object-name>rpm1.L2</object-name>
		<object-name>rpm1.L3</object-name>
		<condition>
			<equals>
				<property>/controls/switches/lp-cock-left</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>rpm.R</object-name>
		<object-name>rpm1.R1</object-name>
		<object-name>rpm1.R2</object-name>
		<object-name>rpm1.R3</object-name>
		<condition>
			<equals>
				<property>/controls/switches/lp-cock-right</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>rpm.L</object-name>
		<property>engines/engine[0]/rpm</property>
		<factor>2.25</factor>
		<center>
			<x-m>0.0</x-m>
			<y-m>-0.044</y-m>
			<z-m>0.051</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>rpm.R</object-name>
		<property>engines/engine[1]/rpm</property>
		<factor>2.25</factor>
		<center>
			<x-m>0.0</x-m>
			<y-m>0.046</y-m>
			<z-m>0.051</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>rpm1.L1</object-name>
		<property>engines/engine[0]/rpm</property>
		<factor>0.001</factor>
		<step>100.0</step>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>rpm1.L2</object-name>
		<property>engines/engine[0]/rpm</property>
		<factor>0.01</factor>
		<step>10.0</step>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>rpm1.L3</object-name>
		<property>engines/engine[0]/rpm</property>
		<factor>0.1</factor>
		<step>1.0</step>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>rpm1.R1</object-name>
		<property>engines/engine[1]/rpm</property>
		<factor>0.001</factor>
		<step>100.0</step>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>rpm1.R2</object-name>
		<property>engines/engine[1]/rpm</property>
		<factor>0.01</factor>
		<step>10.0</step>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>rpm1.R3</object-name>
		<property>engines/engine[1]/rpm</property>
		<factor>0.1</factor>
		<step>1.0</step>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>rpm.L.001</object-name>
		<property>engines/engine[0]/egt-degf</property>
		<factor>0.15</factor>
		<center>
			<x-m>-0.00</x-m>
			<y-m>-0.0448</y-m>
			<z-m>-0.0271</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>rpm.R.001</object-name>
		<property>engines/engine[1]/egt-degf</property>
		<factor>0.15</factor>
		<center>
			<x-m>-0.00</x-m>
			<y-m>0.046</y-m>
			<z-m>-0.0273</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>nozzle.L3</object-name>
		<property>sim/multiplay/generic/float[9]</property>
		<factor>0.1</factor>
		<step>1.0</step>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>nozzle.R3</object-name>
		<property>sim/multiplay/generic/float[10]</property>
		<factor>0.1</factor>
		<step>1.0</step>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<condition>
			<property>/sim/rendering/rembrandt/enabled</property>
		</condition>
		<object-name>Circle</object-name>
	</effect>
	<animation>
		<type>material</type>
		<object-name>Circle</object-name>
		<object-name>rpm.R</object-name>
		<object-name>rpm.L</object-name>
		<object-name>rpm.L.001</object-name>
		<object-name>rpm.L.002</object-name>
		<object-name>rpm.L.003</object-name>
		<object-name>rpm.R.001</object-name>
		<object-name>rpmcover.L</object-name>
		<object-name>rpmcover.L.001</object-name>
		<object-name>rpmcover.L.002</object-name>
		<object-name>rpmcover.L.003</object-name>
		<object-name>rpmcover.R</object-name>
		<object-name>rpmcover.R.001</object-name>
		<emission>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
			<factor-prop>/instrumentation/illumination/LCD</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>material</type>
		<object-name>rpm1.L1</object-name>
		<object-name>rpm1.L2</object-name>
		<object-name>rpm1.L3</object-name>
		<object-name>rpm1.R1</object-name>
		<object-name>rpm1.R2</object-name>
		<object-name>rpm1.R3</object-name>
		<object-name>nozzle.L1</object-name>
		<object-name>nozzle.L2</object-name>
		<object-name>nozzle.L3</object-name>
		<object-name>nozzle.R1</object-name>
		<object-name>nozzle.R2</object-name>
		<object-name>nozzle.R3</object-name>
		<emission>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
			<factor-prop>/instrumentation/illumination/LCD</factor-prop>
		</emission>
	</animation>
	<text>
		<name>fuel</name>
		<font>lcd.txf</font>
		<character-size type="double">0.0065</character-size>
		<offsets>
			<pitch-deg>90</pitch-deg>
			<heading-deg>0</heading-deg>
			<roll-deg>90</roll-deg>
			<x-m>0.022</x-m>
			<y-m>-0.057</y-m>
			<z-m>-0.085</z-m>
		</offsets>
		<type type="string">literal</type>
		<text type="string">FUEL</text>
	</text>
	<text>
		<name>engine</name>
		<font>lcd.txf</font>
		<character-size type="double">0.0065</character-size>
		<offsets>
			<pitch-deg>90</pitch-deg>
			<heading-deg>0</heading-deg>
			<roll-deg>90</roll-deg>
			<x-m>0.022</x-m>
			<y-m>-0.032</y-m>
			<z-m>-0.085</z-m>
		</offsets>
		<type type="string">literal</type>
		<text type="string">ENG</text>
	</text>
	<text>
		<name>hydraulic</name>
		<font>lcd.txf</font>
		<character-size type="double">0.0065</character-size>
		<offsets>
			<pitch-deg>90</pitch-deg>
			<heading-deg>0</heading-deg>
			<roll-deg>90</roll-deg>
			<x-m>0.022</x-m>
			<y-m>-0.007</y-m>
			<z-m>-0.085</z-m>
		</offsets>
		<type type="string">literal</type>
		<text type="string">HYD</text>
	</text>
	<text>
		<name>waypoint</name>
		<font>lcd.txf</font>
		<character-size type="double">0.006</character-size>
		<offsets>
			<pitch-deg>90</pitch-deg>
			<heading-deg>0</heading-deg>
			<roll-deg>90</roll-deg>
			<x-m>0.022</x-m>
			<y-m>0.016</y-m>
			<z-m>-0.085</z-m>
		</offsets>
		<type type="string">literal</type>
		<text type="string">STOR</text>
	</text>
	<text>
		<name>radar</name>
		<font>lcd.txf</font>
		<character-size type="double">0.005</character-size>
		<offsets>
			<pitch-deg>90</pitch-deg>
			<heading-deg>0</heading-deg>
			<roll-deg>90</roll-deg>
			<x-m>0.022</x-m>
			<y-m>0.039</y-m>
			<z-m>-0.085</z-m>
		</offsets>
		<type type="string">literal</type>
		<text type="string">RADAR</text>
	</text>
	<text>
		<name>eadi</name>
		<font>lcd.txf</font>
		<character-size type="double">0.0065</character-size>
		<offsets>
			<pitch-deg>90</pitch-deg>
			<heading-deg>0</heading-deg>
			<roll-deg>90</roll-deg>
			<x-m>0.022</x-m>
			<y-m>-0.096</y-m>
			<z-m>-0.056</z-m>
		</offsets>
		<type type="string">literal</type>
		<text type="string">EADI</text>
	</text>
	<text>
		<name>navpos1</name>
		<font>lcd.txf</font>
		<character-size type="double">0.006</character-size>
		<offsets>
			<pitch-deg>90</pitch-deg>
			<heading-deg>0</heading-deg>
			<roll-deg>90</roll-deg>
			<x-m>0.022</x-m>
			<y-m>0.080</y-m>
			<z-m>0.062</z-m>
		</offsets>
		<type type="string">literal</type>
		<text type="string">NAV</text>
	</text>
	<text>
		<name>navpos2</name>
		<font>lcd.txf</font>
		<character-size type="double">0.006</character-size>
		<offsets>
			<pitch-deg>90</pitch-deg>
			<heading-deg>0</heading-deg>
			<roll-deg>90</roll-deg>
			<x-m>0.022</x-m>
			<y-m>0.080</y-m>
			<z-m>0.055</z-m>
		</offsets>
		<type type="string">literal</type>
		<text type="string">DISP</text>
	</text>
	<animation>
		<type>material</type>
		<object-name>fuel</object-name>
		<object-name>engine</object-name>
		<object-name>hydraulic</object-name>
		<object-name>waypoint</object-name>
		<object-name>radar</object-name>
		<object-name>eadi</object-name>
		<object-name>norm</object-name>
		<object-name>navpos1</object-name>
		<object-name>navpos2</object-name>
		<diffuse>
			<red>0.1</red>
			<green>0.3</green>
			<blue>0.1</blue>
		</diffuse>
		<emission>
			<red>0.4</red>
			<green>0.8</green>
			<blue>0.4</blue>
			<factor-prop>/instrumentation/illumination/LCD</factor-prop>
		</emission>
	</animation>
</PropertyList>
