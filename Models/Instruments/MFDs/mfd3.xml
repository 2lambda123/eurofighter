<?xml version="1.0"?>
<PropertyList>
	<model>
		<name>mfd-buttons-off</name>
		<path>Aircraft/eurofighter/Models/Instruments/MFDs/buttons-off.xml</path>
	</model>

	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<condition>
			<property>sim/rendering/rembrandt/enabled</property>
		</condition>
		<object-name>power</object-name>
	</effect>

	<animation>
		<type>select</type>
		<object-name>mfd-buttons-off</object-name>
		<condition>
			<or>
				<less-than>
					<property>/systems/typhoon-electrical/outputs/avionics/MFHDD[0]</property>
					<value>26</value>
				</less-than>
				<equals>
					<property>/instrumentation/MFD[2]/serviceable</property>
					<value>false</value>
				</equals>
				<equals>
					<property>/instrumentation/MFD[2]/stand-by</property>
					<value>true</value>
				</equals>
			</or>
		</condition>
	</animation>

    <!-- main screen MFD3 -->		
	
