<?xml version="1.0" encoding="utf-8"?>

<!-- Manual Data Entry Facility - MIDS Page -->
<PropertyList>
	<path>softkeys.ac</path>
	<animation>
		<object-name>home-label</object-name>
		<object-name>comm-label</object-name>
		<object-name>atk-label</object-name>
		<object-name>iff-label</object-name>
		<type>material</type>
		<diffuse>
			<red>0.1</red>
			<green>0.3</green>
			<blue>0.1</blue>
		</diffuse>
		<emission>
			<red>0.4</red>
			<green>0.8</green>
			<blue>0.4</blue>
			<factor-prop>/instrumentation/illumination/glareshield-backlight</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>select</type>
		<object-name>softkeys.01.off</object-name>
  <!-- out when FALSE -->
		<condition>
			<not-equals>
				<property>/instrumentation/MIDS/page-selected</property>
				<value>home</value>
			</not-equals>
		</condition>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>softkeys.01.on</object-name>
		<object-name>softkeys.01.off</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>/instrumentation/MIDS/page-selected</property>
				<value>home</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>select</type>
		<object-name>softkeys.02.off</object-name>
  <!-- out when FALSE -->
		<condition>
			<not-equals>
				<property>/instrumentation/MIDS/page-selected</property>
				<value>comm</value>
			</not-equals>
		</condition>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>softkeys.02.on</object-name>
		<object-name>softkeys.02.off</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>/instrumentation/MIDS/page-selected</property>
				<value>comm</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>select</type>
		<object-name>softkeys.03.off</object-name>
  <!-- out when FALSE -->
		<condition>
			<not-equals>
				<property>/instrumentation/MIDS/page-selected</property>
				<value>atk</value>
			</not-equals>
		</condition>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>softkeys.03.on</object-name>
		<object-name>softkeys.03.off</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>/instrumentation/MIDS/page-selected</property>
				<value>atk</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>select</type>
		<object-name>softkeys.05.off</object-name>
  <!-- out when FALSE -->
		<condition>
			<not-equals>
				<property>/instrumentation/MIDS/page-selected</property>
				<value>iff</value>
			</not-equals>
		</condition>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>softkeys.05.on</object-name>
		<object-name>softkeys.05.off</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>/instrumentation/MIDS/page-selected</property>
				<value>iff</value>
			</binding>
		</action>
	</animation>
	<text>
		<name>home-label</name>
		<font>lcd.txf</font>
		<character-size type="double">0.005</character-size>
		<offsets>
			<x-m>3.525</x-m>
			<y-m>-0.1675</y-m>
			<z-m>1.073</z-m>
		</offsets>
		<axis-alignment>yz-plane</axis-alignment>
		<type type="string">literal</type>
		<text type="string">HOME</text>
		<draw-text type="bool">true</draw-text>
		<alignment>center-center</alignment>
	</text>
	<text>
		<name>comm-label</name>
		<font>lcd.txf</font>
		<character-size type="double">0.005</character-size>
		<offsets>
			<x-m>3.525</x-m>
			<y-m>-0.144</y-m>
			<z-m>1.073</z-m>
		</offsets>
		<axis-alignment>yz-plane</axis-alignment>
		<type type="string">literal</type>
		<text type="string">COMM</text>
		<draw-text type="bool">true</draw-text>
		<alignment>center-center</alignment>
	</text>
	<text>
		<name>atk-label</name>
		<font>lcd.txf</font>
		<character-size type="double">0.005</character-size>
		<offsets>
			<x-m>3.525</x-m>
			<y-m>-0.1215</y-m>
			<z-m>1.073</z-m>
		</offsets>
		<axis-alignment>yz-plane</axis-alignment>
		<type type="string">literal</type>
		<text type="string">ATK</text>
		<draw-text type="bool">true</draw-text>
		<alignment>center-center</alignment>
	</text>
	<text>
		<name>iff-label</name>
		<font>lcd.txf</font>
		<character-size type="double">0.005</character-size>
		<offsets>
			<x-m>3.525</x-m>
			<y-m>-0.1675</y-m>
			<z-m>1.0560</z-m>
		</offsets>
		<axis-alignment>yz-plane</axis-alignment>
		<type type="string">literal</type>
		<text type="string">IFF</text>
		<draw-text type="bool">true</draw-text>
		<alignment>center-center</alignment>
	</text>
</PropertyList>